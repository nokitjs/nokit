{
    "hosts": {},
    "port": 8000,
    "defaults": {
        "html": "index.html",
        "nsp": "index.nsp"
    },
    "headers": {},
    "handlers": {
        "*::static": "$./handlers/static",
        "^/-rc-/::rc": "$./handlers/resource",
        "^/favicon.icon$::favicon": "$./handlers/favicon",
        "^/favicon.ico$::favicon": "$./handlers/favicon",
        ".nsp$::nsp": "$./handlers/nsp",
        ".nsh$::nsh": "$./handlers/nsh"
    },
    "parsers": {
        "*": "$./parsers/urlencoded",
        "multipart/form-data": "$./parsers/multipart",
        "application/x-www-form-urlencoded": "$./parsers/urlencoded",
        "text/plain": "$./parsers/plain",
        "text/json": "$./parsers/json",
        "text/xml": "$./parsers/xml",
        "application/json": "$./parsers/json",
        "application/xml": "$./parsers/xml"
    },
    "global": "./global",
    "filters": {
        "^/::session": "",
        "^/::log": ""
    },
    "template": {
        "pages": {
            "500": "$./templates/500.html",
            "404": "$./templates/404.html",
            "403": "$./templates/403.html",
            "405": "$./templates/405.html",
            "explore": "$./templates/explore.html"
        },
        "compress": true
    },
    "folders": {
        "public": "./public"
    },
    "cache": {
        "match": {
            "html": ".html$",
            "htm": ".htm$",
            "css": ".css$",
            "js": ".js$",
            "json": ".json$",
            "xml": ".xml$",
            "svg": ".svg$",
            "txt": ".txt$",
            "text": ".text$",
            "csv": ".csv$",
            "iocn": ".icon$",
            "ico": ".ico$",
            "less": ".less$",
            "sass": ".sass$",
            "markdown": ".md$",
            "png": ".png$",
            "jpg": ".jpg$",
            "jpeg": ".jpeg$",
            "gif": ".gif$",
            "bmp": ".bmp$",
            "swf": ".swf$",
            "ttf": ".ttf$",
            "woff": ".woff$",
            "eot": ".eot"
        },
        "maxAge": 1800,
        "lastModified": true
    },
    "denys": {
        "nsp-js": ".nsp.js$",
        "nsp-html": ".nsp.html$",
        "nsh-js": ".nsh.js$",
        "web-json": "web.json$",
        "global-js": "global.js$",
        "package-json": "package.json$",
        "tp": ".tp$",
        "tp-html": ".tp.html$",
        "tp-htm": ".tp.htm$",
        "log": ".log$"
    },
    "session": {
        "provider": "$./providers/session-process",
        "state": true,
        "timeout": 1800
    },
    "compress": {
        "html": ".html$",
        "htm": ".htm$",
        "css": ".css$",
        "js": ".js$",
        "json": ".json$",
        "xml": ".xml$",
        "svg": ".svg$",
        "txt": ".txt$",
        "text": ".text$",
        "markdown": ".md$",
        "csv": ".csv$"
    },
    "displayDir": true,
    "log": {
        "provider": "$./providers/logger-file",
        "path": "./logs"
    },
    "mimeType": {
        "*": "application/octet-stream",
        ".html": "text/html",
        ".htm": "text/html",
